#!/usr/bin/env python

import sys

import clusterbank.model

def run (argv=None):
    clusterbank.model.metadata.create_all()
    clusterbank.model.Session.flush()
    clusterbank.model.Session.commit()

if __name__ == "__main__":
    try:
        run()
    except KeyboardInterrupt:
        sys.exit(1)
    except Exception, e:
        print >> sys.stderr, e
        sys.exit(1)
